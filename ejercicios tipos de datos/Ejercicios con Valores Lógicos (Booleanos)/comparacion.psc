//Escribe un programa que compare dos números y muestre Verdadero si son iguales.
Algoritmo comparacion
	Definir num1 Como entero;
	Definir num2 Como Entero;

	Escribir "Comparacion de dos numeros"
	Escribir "ingresa primer numero";

	leer num1;
	Escribir "ingresa segundo numero";
	leer num2;
	
	si num1==num2 Entonces
		Escribir "Verdadero";
	SiNo
		Escribir "Falso";
	FinSi
	
FinAlgoritmo
